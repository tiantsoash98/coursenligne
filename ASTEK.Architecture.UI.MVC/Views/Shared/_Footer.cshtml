@model ASTEK.Architecture.UI.MVC.Models.BaseViewModel

<footer class="personal-footer">
    <div class="container">
        <div class="row">

            <div class="col-lg-4 col-md-6">
                <div class="footer-section">

                    <a class="logo" href="@Url.Action("Index", "Home")">
                        <img src="@Url.Content(Model.BaseFileUrl + Model.ImageFolder + "logo.png")" alt="Logo Image">
                    </a>
                    <p class="copyright">@UIStrings.App_Title 2018. @UIStrings.Footer_ReservedRight.</p>
                    @*<p class="copyright">Designed by <a href="https://colorlib.com" target="_blank">Colorlib</a></p>*@
                    @Html.Partial("_Social")
                </div><!-- footer-section -->
            </div><!-- col-lg-4 col-md-6 -->

            @Html.Partial("_FooterStudy", Model.Studies)

            @Html.Action("Culture")

        </div><!-- row -->
    </div><!-- container -->
</footer>